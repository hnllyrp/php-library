{
    "name": "hnllyrp/php-support",
    "description": "The php common library support. Array、String、Time、Util",
    "keywords": [
        "php",
        "library",
        "Util"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hnllyrp",
            "email": "hnllyrp@126.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*"
    },
    "autoload": {
        "files": [
            "src/helper.php"
        ],
        "psr-4": {
            "Hnllyrp\\PhpSupport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hnllyrp\\PhpSupport\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
